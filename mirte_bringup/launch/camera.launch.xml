<?xml version="1.0" encoding="UTF-8"?>
<launch version="0.1.0">
  <node pkg="usb_cam" name="webcam" exec="usb_cam_node_exe">
    <param name="pixel_format" value="yuyv"/>
    <param name="video_device" value="/dev/video1"/>
  </node>
   
  <!-- The video web server is broken untill https://github.com/RobotWebTools/web_video_server/pull/136 is merged,
    since the port parameter cannot be changed and port is already used (Now on the forked branch) -->
   <node pkg="web_video_server" name="web_video_server" exec="web_video_server">
    <param name="default_transport" value="theora"/>
    <param name="port" value="8181"/>
  </node>
</launch>